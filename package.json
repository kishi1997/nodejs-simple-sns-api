{
  "name": "sample-api",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ts-node": "ts-node --files -r tsconfig-paths/register",
    "dev": "ts-node-dev --respawn --transpile-only --files -r tsconfig-paths/register ./src/index.ts",
    "lint": "prettier --check src",
    "lint:fix": "prettier --write src",
    "typecheck": "tsc -p . --noEmit",
    "test": "jest --runInBand",
    "typeorm": "TS_NODE_TRANSPILE_ONLY=true yarn ts-node ./node_modules/typeorm/cli.js",
    "start": "node -r ./tsconfig-paths-bootstrap.js dist/src/index.js",
    "console": "NODE_OPTIONS=--experimental-repl-await yarn ts-node console.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "^0.2.29"
  },
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/jest": "^29.2.0",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.13",
    "jest": "^29.2.1",
    "prettier": "^2.2.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.1.3"
  }
}
